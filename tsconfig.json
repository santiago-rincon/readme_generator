{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@icons/*": ["./src/icons/*"],
      "@types": ["./src/types.d.ts"],
      "@pages/*": ["./src/pages/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@hooks/*": ["src/hooks/*"],
      "@store/*": ["src/store/*"],
    }
  }
}
